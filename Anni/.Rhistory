library("dplyr")
library('PerformanceAnalytics') #find correlation chart.Correlation()
library("ggplot2")
library("knitr")
#Read in the data()
med <- read.csv('~/Desktop/Demographic/claims_med_interns_2016-2018.csv')
pharm <- read.csv('~/Desktop/Demographic/claims_pharm_capstone_2016-2018.csv')
#med data analysis
print(colnames(med))
med_revise <- med %>% select("Claim.ID", "Member.Gender", "Age", "Zip..5.digit.",
"Primary.ICD.Diagnosis.Code", "Primary.ICD.Rollup",
"Revenue.Code", "DRG.Code", "CCHG.Grouping", "CCHG.Label",
"Incurred.Year")
icd <- med_revise %>%
group_by(Primary.ICD.Rollup) %>%
distinct(Claim.ID) %>%
summarize(n=n()) %>%
arrange(desc(n))
View(icd)
year <- med_revise %>%
group_by(Incurred.Year) %>%
distinct(Claim.ID) %>%
summarize(n=n()) %>%
arrange(desc(n))
View(year)
View(pharm)
service_line <- pharm %>%
group_by(Service.Line) %>%
distinct(Claim.ID) %>%
summarize(n=n()) %>%
arrange(desc(n))
View(service_line)
employee_type <- pharm %>%
group_by(Employee.Type) %>%
distinct(Claim.ID) %>%
summarize(n=n()) %>%
arrange(desc(n))
View(employee_type)
ggplot(employee_type) + geom_bar(aes(x=Employee.Type, y=n), stat = "identity")
ggplot(employee_type) +
geom_bar(aes(x=Employee.Type, y=n), stat = "identity") +
labs(title = "Employee Type Count", x = "Employee Type", y = "Count")
#Age Distribution
ggplot(med_revise) +
geom_histogram(aes(x=Age), stat="count") +
#Age Distribution
ggplot(med_revise) +
geom_histogram(aes(x=Age), stat="count") +
labs(title = "Age Distribution", x = "Age", y = "Count")
library("dplyr")
library('PerformanceAnalytics') #find correlation chart.Correlation()
library("ggplot2")
library("knitr")
#Read in the data()
med <- read.csv('~/Desktop/Demographic/claims_med_interns_2016-2018.csv')
pharm <- read.csv('~/Desktop/Demographic/claims_pharm_capstone_2016-2018.csv')
#med data analysis
print(colnames(med))
med_revise <- med %>% select("Claim.ID", "Member.Gender", "Age", "Zip..5.digit.",
"Primary.ICD.Diagnosis.Code", "Primary.ICD.Rollup",
"Revenue.Code", "DRG.Code", "CCHG.Grouping", "CCHG.Label",
"Incurred.Year")
#Age Distribution
ggplot(med_revise) +
geom_histogram(aes(x=Age), stat="count") +
labs(title = "Age Distribution", x = "Age", y = "Count")
#Gender Distribution
ggplot(med_revise) +
geom_histogram(aes(x=Member.Gender), stat="count") +
labs(title = "Gender Distribution", x = "Gender", y = "Count")
kable(icd[1:10,])
kable(year)
ggplot(year) +
geom_bar(aes(x=Incurred.Year, y=n), stat="identity") +
labs(title = "Year Counts", x = "Year", y = "Count")
ggplot(med_revise) +
geom_histogram(aes(x=Age, fill=year), stat="count") +
labs(title = "Age Distribution", x = "Age", y = "Count")
ggplot(med_revise, aes(fill=Incurred.Year)) +
geom_histogram(aes(x=Age), stat="count") +
labs(title = "Age Distribution", x = "Age", y = "Count")
#Age Distribution by Year
age_by_year <- med_revise %>% group_by(year) %>%
ggplot(med_revise) +
geom_histogram(aes(x=Age), stat="count") +
labs(title = "Age Distribution", x = "Age", y = "Count") +
facet_wrap(~Incurred.Year)
#Age Distribution by Year
ggplot(med_revise) +
geom_histogram(aes(x=Age), stat="count") +
labs(title = "Age Distribution", x = "Age", y = "Count") +
facet_wrap(~Incurred.Year)
